#1로 만들기
n=int(input())
#n+1개만큼의 0으로 이루어진 배열을 만들어준다.
dp=[0 for i in range(n+1)]

for i in range(2,n+1):
    dp[i]=dp[i-1]+1

    if  i % 2==0:
        if dp[i]>dp[int(i//2)]+1:
            dp[i]=dp[int(i//2)]+1
    if  i % 3==0:
        if dp[i]>dp[int(i//3)]+1:
            dp[i]=dp[int(i//3)]+1
print(dp[n])

