import bisect

x=int(input())
arr=list(map(int,  input().split()))

dp=[arr[0]]# dp를 arr[0]으로 초기화한다.

for i in range(x):
    if arr[i]>dp[-1]:#현재 위치(i)가 이전 위치의 원소들보다 크면 dp에 추가한다.
        dp.append(arr[i])
    else:#현재 위치(i)가 이전 위치의 원소보다 작거나 같으면
        idx=bisect.bisect_left(dp,  arr[i])#bisect.bisect_left로 이전 위치의 원소 중 가장 큰 원소의 index값을 구한
        dp[idx]=arr[i]# dp의 index 원소를 arr[i]로 바꿔준다

print(len(dp))
