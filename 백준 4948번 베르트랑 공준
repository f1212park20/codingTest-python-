import math

def isprime(num):
    if num==1:
        return False
    for i in range(2,int(math.sqrt(num))+1):
        if num % i==0:
            return False
    return True    

li=list(range(2,246912 ))
#미리 123456의 2배인 246912 사이에 있는 소수를 구해 그 리스트에서 비교함
prime_li=[]
for i in li:
    if isprime(i):
        prime_li.append(i)

while(1):
    answer=0
    n=int(input())
    if n==0:
        break

    for i in prime_li:
        if n < i <= n*2:
            answer+=1
    print(answer)       
    
