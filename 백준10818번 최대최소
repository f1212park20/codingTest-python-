import java.util.Arrays;
import java.util.Scanner;
 
public class Main {
	public static void main(String[] args) {
    
		Scanner in = new Scanner(System.in);
        
		int N = in.nextInt();
		int[] arr = new int[N];
        
		for (int i = 0; i < N; i++) {
			arr[i] = in.nextInt();
		}
        
		in.close();
		Arrays.sort(arr);
    //이 메소드는 배열에 저장된 원소 값을 오름차순으로 정렬해주는 메소드다
		System.out.print(arr[0] + " " + arr[N - 1]);
	}
}
